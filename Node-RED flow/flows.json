    {
        "id": "402b8abd.a667d4",
        "type": "function",
        "z": "2d7c70cb.16397",
        "name": "Time Diff Calc",
        "func": "// Create a Date object from the payload and remove initial time\n// Friday, June 14, 2019 2:00:00 PM GMT+02:00\nvar date    = (msg.payload - 1560520800000) / 1000;\n\n//Truncate to int if divded by 1000 to show sec\ndate = Math.floor(date);\n\nvar seconds = parseInt((date)%60);\nvar minutes = parseInt((date/60)%60);\nvar hours   = parseInt((date/(60*60))%24);\n\nhours = (hours < 10) ? \"0\" + hours : hours;\nminutes = (minutes < 10) ? \"0\" + minutes : minutes;\nseconds = (seconds < 10) ? \"0\" + seconds : seconds;\n\nvar date_ret = hours + \":\" + minutes + \":\" + seconds;\n\nmsg.payload = date_ret;\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "x": 456.5,
        "y": 324.79998779296875,
        "wires": [
